<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragHome">
    <fragment
        android:id="@+id/fragHome"
        android:name="com.thedhobighat.FragHome"
        android:label="frag_home"
        tools:layout="@layout/frag_home" >
        <action
            android:id="@+id/action_fragHome_to_fragChooseItems"
            app:destination="@id/fragChooseItems"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_out_to_left"
            app:popEnterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_left" />
        <action
            android:id="@+id/action_fragHome_to_orderHistory"
            app:destination="@id/orderHistory" />
        <action
            android:id="@+id/action_fragHome_to_userProfile"
            app:destination="@id/userProfile" />
    </fragment>
    <fragment
        android:id="@+id/fragChooseItems"
        android:name="com.thedhobighat.FragChooseItems"
        android:label="frag_choose_items"
        tools:layout="@layout/frag_choose_items" >
        <action
            android:id="@+id/action_fragChooseItems_to_fragSubmitYourInfo"
            app:destination="@id/fragSubmitYourInfo"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_out_to_left"
            app:popEnterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_left" />
        <action
            android:id="@+id/action_fragChooseItems_to_fragHome"
            app:destination="@id/fragHome"
            app:enterAnim="@anim/slide_in_from_left"
            app:exitAnim="@anim/slide_out_to_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_fragChooseItems_to_orderHistory"
            app:destination="@id/orderHistory" />
        <action
            android:id="@+id/action_fragChooseItems_to_userProfile"
            app:destination="@id/userProfile" />
        <action
            android:id="@+id/action_fragChooseItems_to_laundryBasket"
            app:destination="@id/laundryBasket" />
    </fragment>
    <fragment
        android:id="@+id/fragSubmitYourInfo"
        android:name="com.thedhobighat.FragSubmitYourInfo"
        android:label="frag_submit_your_info"
        tools:layout="@layout/frag_submit_your_info" >
        <action
            android:id="@+id/action_fragSubmitYourInfo_to_fragPayment"
            app:destination="@id/fragPayment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_out_to_left"
            app:popEnterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_left" />
        <action
            android:id="@+id/action_fragSubmitYourInfo_to_fragChooseItems"
            app:destination="@id/fragChooseItems"
            app:enterAnim="@anim/slide_in_from_left"
            app:exitAnim="@anim/slide_out_to_right"
            app:popEnterAnim="@anim/slide_in_from_left"
            app:popExitAnim="@anim/slide_out_to_right" />
        <action
            android:id="@+id/action_fragSubmitYourInfo_to_orderHistory"
            app:destination="@id/orderHistory" />
        <action
            android:id="@+id/action_fragSubmitYourInfo_to_userProfile"
            app:destination="@id/userProfile" />
        <action
            android:id="@+id/action_fragSubmitYourInfo_to_fragHome"
            app:destination="@id/fragHome" />
        <action
            android:id="@+id/action_fragSubmitYourInfo_to_laundryBasket"
            app:destination="@id/laundryBasket" />
    </fragment>
    <fragment
        android:id="@+id/fragPayment"
        android:name="com.thedhobighat.FragPayment"
        android:label="frag_payment"
        tools:layout="@layout/frag_payment" >
        <action
            android:id="@+id/action_fragPayment_to_fragTrackYourOrder"
            app:destination="@id/fragTrackYourOrder"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_out_to_left"
            app:popEnterAnim="@anim/slide_in_from_right"
            app:popExitAnim="@anim/slide_out_to_left" />
        <action
            android:id="@+id/action_fragPayment_to_fragSubmitYourInfo"
            app:destination="@id/fragSubmitYourInfo"
            app:enterAnim="@anim/slide_in_from_left"
            app:exitAnim="@anim/slide_out_to_right"
            app:popEnterAnim="@anim/slide_in_from_left"
            app:popExitAnim="@anim/slide_out_to_right" />
        <action
            android:id="@+id/action_fragPayment_to_orderHistory"
            app:destination="@id/orderHistory" />
        <action
            android:id="@+id/action_fragPayment_to_userProfile"
            app:destination="@id/userProfile" />
        <action
            android:id="@+id/action_fragPayment_to_fragHome"
            app:destination="@id/fragHome" />
        <action
            android:id="@+id/action_fragPayment_to_laundryBasket"
            app:destination="@id/laundryBasket" />
    </fragment>
    <fragment
        android:id="@+id/fragTrackYourOrder"
        android:name="com.thedhobighat.FragTrackYourOrder"
        android:label="frag_track_your_order"
        tools:layout="@layout/frag_track_your_order" >
        <action
            android:id="@+id/action_fragTrackYourOrder_to_fragPayment"
            app:destination="@id/fragPayment"
            app:enterAnim="@anim/slide_in_from_left"
            app:exitAnim="@anim/slide_out_to_right"
            app:popEnterAnim="@anim/slide_in_from_left"
            app:popExitAnim="@anim/slide_out_to_right" />
        <action
            android:id="@+id/action_fragTrackYourOrder_to_orderHistory"
            app:destination="@id/orderHistory" />
        <action
            android:id="@+id/action_fragTrackYourOrder_to_userProfile"
            app:destination="@id/userProfile" />
        <action
            android:id="@+id/action_fragTrackYourOrder_to_fragHome"
            app:destination="@id/fragHome" />
        <action
            android:id="@+id/action_fragTrackYourOrder_to_laundryBasket"
            app:destination="@id/laundryBasket" />
    </fragment>
    <fragment
        android:id="@+id/orderHistory"
        android:name="com.thedhobighat.OrderHistory"
        android:label="frag_order_history"
        tools:layout="@layout/frag_order_history" >
        <action
            android:id="@+id/action_orderHistory_to_userProfile"
            app:destination="@id/userProfile" />
        <action
            android:id="@+id/action_orderHistory_to_laundryBasket"
            app:destination="@id/laundryBasket" />
    </fragment>
    <fragment
        android:id="@+id/userProfile"
        android:name="com.thedhobighat.UserProfile"
        android:label="frag_user_profile"
        tools:layout="@layout/frag_user_profile" >
        <action
            android:id="@+id/action_userProfile_to_orderHistory"
            app:destination="@id/orderHistory" />
        <action
            android:id="@+id/action_userProfile_to_laundryBasket"
            app:destination="@id/laundryBasket" />
    </fragment>
    <fragment
        android:id="@+id/laundryBasket"
        android:name="com.thedhobighat.LaundryBasket"
        android:label="frag_laundry_basket"
        tools:layout="@layout/frag_laundry_basket" >
        <action
            android:id="@+id/action_laundryBasket_to_orderHistory"
            app:destination="@id/orderHistory" />
        <action
            android:id="@+id/action_laundryBasket_to_userProfile"
            app:destination="@id/userProfile" />
    </fragment>
</navigation>